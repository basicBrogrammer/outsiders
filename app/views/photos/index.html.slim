.home_container
  ul.small-block-grid-2.medium-block-grid-3.large-block-grid-4
    - @photos.each do |photo|
        li
          = image_tag photo.image.remote_url if photo.image_stored?
          = button_to "Make this your profpic?", {action: "update", id: photo.id, photo: {profile: 1}}, method: :put
 
